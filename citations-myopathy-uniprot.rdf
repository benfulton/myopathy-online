<author>Bahl A.</author>
<author>Evans A.</author>
<author>Osman E.</author>
<author>Syrris P.</author>
<author>Gorman G.</author>
<author>Farrell M.</author>
<author>Holton J.L.</author>
<author>Hanna M.G.</author>
<author>Hughes S.</author>
<author>Elliott P.M.</author>
<author>Macrae C.A.</author>
<author>McKenna W.J.</author>
<skos:exactMatch rdf:resource="http://purl.uniprot.org/pubmed/15766830"/>
